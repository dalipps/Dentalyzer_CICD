{
	"name": "dentalyzer",
	"version": "0.0.0",
	"scripts": {
		"start": "ng serve --hmr",
		"lint:fix": "eslint --fix src/**/*.ts",
		"lint": "eslint src/**/*.ts",
		"format:fix": "npx prettier --write src/**/*.{ts,js,json,html,css,scss,md}",
		"format": "npx prettier --check src/**/*.{ts,js,json,html,css,scss,md}",
		"test": "jest --config ./jest.config.js",
		"test:watch": "jest -o --watch --config ./jest.config.js",
		"i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/de.json --sort --clean --format json --marker _",
		"build": "ng build",
		"build:prod": "ng build --configuration production",
		"watch": "ng build --watch --configuration development"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^15.1.4",
		"@angular/common": "^15.1.4",
		"@angular/compiler": "^15.1.4",
		"@angular/core": "^15.1.4",
		"@angular/forms": "^15.1.4",
		"@angular/platform-browser": "^15.1.4",
		"@angular/platform-browser-dynamic": "^15.1.4",
		"@angular/router": "^15.1.4",
		"@ngx-translate/core": "^14.0.0",
		"@ngx-translate/http-loader": "^7.0.0",
		"primeflex": "^3.3.0",
		"primeicons": "^6.0.1",
		"primeng": "^15.2.0",
		"rxjs": "~7.5.0",
		"tslib": "^2.5.0",
		"zone.js": "~0.11.8"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^15.1.5",
		"@angular/cli": "^15.1.5",
		"@angular/compiler-cli": "^15.1.4",
		"@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
		"@testing-library/angular": "^13.1.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@types/babel-generator": "^6.25.5",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"@vendure/ngx-translate-extract": "^8.0.3",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"jest": "^28.1.3",
		"jest-preset-angular": "^12.2.6",
		"prettier": "^2.8.3",
		"prettier-eslint": "^15.0.1",
		"typescript": "~4.8.4"
	}
}
