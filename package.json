{
	"name": "dentalyzer",
	"version": "0.0.0",
	"scripts": {
		"start": "ng serve --hmr",
		"lint:fix": "eslint --fix src",
		"lint": "eslint src",
		"format:fix": "npx prettier --write src",
		"format": "npx prettier --check src",
		"test": "jest --config ./jest.config.js",
		"test:watch": "jest -o --watch --config ./jest.config.js",
		"i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/de.json --sort --clean --format namespaced-json --marker t",
		"build": "ng build",
		"build:prod": "ng build --configuration production",
		"watch": "ng build --watch --configuration development"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^15.2.6",
		"@angular/cdk": "^15.2.6",
		"@angular/common": "^15.2.6",
		"@angular/compiler": "^15.2.6",
		"@angular/core": "^15.2.6",
		"@angular/forms": "^15.2.6",
		"@angular/material": "^15.2.6",
		"@angular/platform-browser": "^15.2.6",
		"@angular/platform-browser-dynamic": "^15.2.6",
		"@angular/router": "^15.2.6",
		"@angular/service-worker": "^15.2.6",
		"@ngrx/effects": "^15.4.0",
		"@ngrx/entity": "^15.4.0",
		"@ngrx/store": "^15.4.0",
		"@ngrx/store-devtools": "^15.4.0",
		"@ngx-translate/core": "^14.0.0",
		"@ngx-translate/http-loader": "^7.0.0",
		"dexie": "^3.2.3",
		"lodash-es": "^4.17.21",
		"pdf-lib": "^1.17.1",
		"rxjs": "~7.5.0",
		"tailwindcss": "^3.3.2",
		"three": "^0.151.3",
		"three-spritetext": "^1.8.1",
		"tslib": "^2.5.0",
		"zone.js": "~0.11.8"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^15.2.5",
		"@angular/cli": "^15.2.5",
		"@angular/compiler-cli": "^15.2.6",
		"@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
		"@testing-library/angular": "^13.1.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@types/babel-generator": "^6.25.5",
		"@types/lodash-es": "^4.17.7",
		"@types/three": "^0.150.1",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"@vendure/ngx-translate-extract": "^8.0.3",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"jest": "^28.1.3",
		"jest-preset-angular": "^12.2.6",
		"prettier": "^2.8.3",
		"prettier-eslint": "^15.0.1",
		"typescript": "~4.8.4"
	}
}
